{
	"name": "alicialonso-api",
	"version": "2.0.0",
	"description": "AliciAlonso GraphQL API",
	"repository": {
		"type": "git",
		"url": "https://github.com/evelasko/aaxapi.git"
	},
	"author": "Enrique Velasco",
	"license": "MIT",
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"scripts": {
		"dev": "env-cmd -f ./env/dev.env ts-node-dev -r tsconfig-paths/register --no-notify --respawn --transpileOnly ./src",
		"build": "tsc",
		"generate:photon": "yarn -s prisma2 generate",
		"generate:nexus": "ts-node -r tsconfig-paths/register --transpile-only ./src/schema",
		"postinstall": "yarn -s generate:photon"
	},
	"_moduleAliases": {
		"@constants": "dist/constants/",
		"@helpers": "dist/helpers/",
		"@libs": "dist/libs/",
		"@permissions": "dist/permissions/",
		"@queue": "dist/queue/",
		"@schema": "dist/schema/",
		"@server/*": "dist/server/*",
		"@aatypes": "dist/types/"
	},
	"prettier": {
		"printWidth": 120,
		"tabWidth": 4,
		"singleQuote": true,
		"semi": false,
		"trailingComma": "none"
	},
	"lint-staged": {
		"*.ts": [
			"eslint"
		]
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"bull": "^3.11.0",
		"connect-redis": "^4.0.3",
		"cors": "^2.8.5",
		"env-cmd": "^10.0.1",
		"express": "^4.17.1",
		"express-handlebars": "^3.1.0",
		"express-session": "^1.17.0",
		"graphql": "^14.5.8",
		"graphql-middleware": "^4.0.1",
		"graphql-shield": "^6.1.0",
		"graphql-yoga": "^1.18.3",
		"handlebars": "^4.4.3",
		"http-proxy-middleware": "^0.20.0",
		"ioredis": "^4.14.1",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.15",
		"mailgun-nodemailer-transport": "^1.2.1",
		"module-alias": "^2.2.2",
		"moment": "^2.24.0",
		"nexus": "0.12.0-beta.14",
		"nexus-prisma": "^0.5.0",
		"node-cache": "^4.2.1",
		"node-redsys-api": "^0.0.5",
		"nodemailer": "^6.3.1",
		"redis": "^2.8.0",
		"type-cacheable": "^1.2.1",
		"typescript-rest": "^2.2.2",
		"uuid": "^3.3.3",
		"validator": "^11.1.0",
		"yup": "^0.27.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^1.8.1",
		"@graphql-codegen/introspection": "^1.8.1",
		"@graphql-codegen/typescript": "^1.8.1",
		"@graphql-codegen/typescript-document-nodes": "^1.8.1",
		"@graphql-codegen/typescript-graphql-request": "^1.8.1",
		"@types/bcryptjs": "^2.4.2",
		"@types/bull": "^3.10.3",
		"@types/connect-redis": "^0.0.11",
		"@types/express-handlebars": "^0.0.33",
		"@types/express-session": "^1.15.15",
		"@types/graphql": "^14.5.0",
		"@types/http-proxy-middleware": "^0.19.3",
		"@types/ioredis": "^4.0.18",
		"@types/jest": "^24.0.19",
		"@types/jsonwebtoken": "^8.3.5",
		"@types/lodash": "^4.14.144",
		"@types/module-alias": "^2.0.0",
		"@types/node": "^12.11.1",
		"@types/node-cache": "^4.1.3",
		"@types/node-fetch": "^2.5.2",
		"@types/nodemailer": "^6.2.1",
		"@types/uuid": "^3.4.5",
		"@types/validator": "^10.11.3",
		"@types/ws": "^6.0.3",
		"@types/yup": "^0.26.24",
		"@typescript-eslint/eslint-plugin": "^2.4.0",
		"@typescript-eslint/parser": "^2.4.0",
		"detect-port": "^1.3.0",
		"eslint": "^6.5.1",
		"eslint-config-google": "^0.14.0",
		"eslint-config-prettier": "^6.4.0",
		"eslint-import-resolver-typescript": "^2.0.0",
		"eslint-plugin-jest": "^22.19.0",
		"eslint-plugin-json": "^1.4.0",
		"eslint-plugin-prettier": "^3.1.1",
		"graphql-request": "^1.8.2",
		"jest": "^24.9.0",
		"prettier": "^1.18.2",
		"prisma2": "2.0.0-preview014.2",
		"ts-jest": "^24.1.0",
		"ts-node": "8.4.1",
		"ts-node-dev": "^1.0.0-pre.43",
		"tsconfig-paths": "^3.9.0",
		"typescript": "3.6.4"
	}
}
